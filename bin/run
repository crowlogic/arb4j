#!/bin/sh 


BASEDIR=$(dirname $(readlink -f $0))/..
CLASSPATH=`cat $BASEDIR/class.path`
export MALLOC_CONF=" FUCK prof:true,prof_leak:true,lg_prof_sample:1,prof_accum:false,prof_prefix:jeprof.out"
LD_PRELOAD=/usr/local/lib/libjemalloc.so java -Djava.library.path=$BASEDIR -ea -classpath $CLASSPATH:$BASEDIR/target/classes $@



