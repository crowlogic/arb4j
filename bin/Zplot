#!/bin/sh -x


BASEDIR=$(dirname $(readlink -f $0))/..
export ASAN_OPTIONS=detect_leaks=1
export LD_PRELOAD=/usr/lib/x86_64-linux-gnu/libasan.so.6
CLASSPATH=`cat $BASEDIR/class.path`
java -Djava.library.path=$BASEDIR -ea -classpath $CLASSPATH:$BASEDIR/target/classes arb.viz.rendering.ZRenderer $@



