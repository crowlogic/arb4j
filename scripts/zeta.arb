FunctionPlotter.darkStyle=false;
var context = new Context();
var theta = RealFunction.express("theta","im(lnΓ(1/4+I*t/2))-(log(π)/2)*t",context);
var R = RealToComplexFunction.express("z","ζ(1/2+I*t)",context);
var f = RealToComplexFunction.express("f","exp(I*theta(t))",context);
plot( -25, 75, 100*100, R.realPart(), R.imagPart(), f.realPart(), f.imagPart() );
out.format("add Z\n");
var Z = RealFunction.express("Z(t)");
var Ztoo = RealToComplexFunction.express("z(t)*f(t)",context);
plot( -25, 75, 100*100, R.realPart(), R.imagPart(), f.realPart(), f.imagPart(), Z, Ztoo.realPart(), Ztoo.imagPart() );