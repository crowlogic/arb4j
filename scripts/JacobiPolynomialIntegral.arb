var context = new Context();
context.registerFunction("P4",P4);
context.registerSequence("P",P);

var f = RealNullaryFunction.express("int(P(4)(x),x=-1..1)",context);
var h = RealNullaryFunction.express("int(P4(x),x=-1..1)",context);
var g = RealPolynomialNullaryFunction.express("int(P(3),x)",context);